public class Solution {
    public int RemoveDuplicates(int[] nums) {
        //Input: nums = [0,0,1,1,1,2,2,3,3,4]
        //Output: 5, nums = [0,1,2,3,4,_,_,_,_,_]   [1,1,2]
        int j=0;
        for(int i=1;i<nums.Length;i++){            
            if(nums[i]!=nums[j]){
                j++;
                nums[j]=nums[i];
            }
        }
        return j+1;
    }
}
