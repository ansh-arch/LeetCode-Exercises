public class Solution {
    public bool CheckIfExist(int[] arr) {
        
        //Edge Case handle
        if(arr==null || arr.Length==0){
            return false;
        }
        //Looping for condition and result
        for(int i=0;i<arr.Length;i++){
            for(int j=0;j<arr.Length;j++){
                if(i!=j){
                    if(arr[i] == 2 * arr[j]){
                        return true;
                    }
                }
            }
        }
        //If not result
        return false;
    }
}
